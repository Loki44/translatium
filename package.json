{
  "name": "translatium",
  "description": "Translator app for Mac & PC.",
  "version": "7.6.0",
  "main": "public/electron.js",
  "repository": "https://github.com/translatium/translatium",
  "author": {
    "name": "Quang Lam",
    "email": "quang@translatiumapp.com",
    "url": "https://quanglam.me"
  },
  "license": "MPL-2.0",
  "engines": {
    "node": "8"
  },
  "scripts": {
    "start": "cross-env REACT_APP_VERSION=dev REACT_APP_GOOGLE_ENDPOINT=http://localhost:3000 BROWSER=none react-scripts start",
    "electron-dev": "concurrently \"yarn start\" \"wait-on http://localhost:3000 && electron .\"",
    "build": "cross-env REACT_APP_VERSION=7.6 react-scripts build && node add-bom.js",
    "test-ui": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "dist-electron": "npm run build && babel-node packager.js",
    "lint": "eslint src public --ext js"
  },
  "dependencies": {
    "electron-is-dev": "0.3.0",
    "electron-settings": "3.1.2",
    "menubar": "5.2.3"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "classnames": "2.2.5",
    "concurrently": "3.5.0",
    "cross-env": "5.0.5",
    "del": "3.0.0",
    "electron": "1.7.8",
    "electron-builder": "19.32.2",
    "eslint": "4.5.0",
    "eslint-config-airbnb": "15.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.4.0",
    "glob": "7.1.2",
    "history": "4.7.2",
    "material-ui": "1.0.0-beta.12",
    "material-ui-icons": "1.0.0-beta.10",
    "mousetrap": "1.6.1",
    "pouchdb-browser": "6.3.4",
    "prop-types": "15.6.0",
    "react": "16.0.0",
    "react-dom": "16.0.0",
    "react-redux": "5.0.6",
    "react-router": "3.0.5",
    "react-router-redux": "4.0.8",
    "react-scripts": "1.0.14",
    "recordrtc": "5.4.3",
    "redux": "3.7.2",
    "redux-thunk": "2.2.0",
    "wait-on": "2.0.2"
  },
  "homepage": "./",
  "proxy": {
    "/m/translate": {
      "target": "https://translate.google.com",
      "changeOrigin": true
    },
    "/translate_a": {
      "target": "https://translate.google.com",
      "changeOrigin": true
    },
    "/translate_tts": {
      "target": "https://translate.google.com",
      "changeOrigin": true,
      "ws": true
    },
    "/speech-api": {
      "target": "https://www.google.com",
      "changeOrigin": true
    }
  }
}
